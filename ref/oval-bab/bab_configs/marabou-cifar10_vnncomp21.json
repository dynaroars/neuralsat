{
  "bounding": {
    "nets": [
      {
        "bounding_algorithm": "propagation",
        "params": {
          "nb_steps": 5,
          "initial_step_size": 1e-1,
          "step_size_decay": 0.98,
          "betas": [0.9, 0.999],
          "joint_ib": false
        },
        "type": "beta-crown",
        "auto_iters": true,
        "batch_size": 600,
        "max_solver_batch": 12000
      },
      {
        "bounding_algorithm": "dual-anderson",
        "params": {
            "bigm": "init",
            "cut": "only",
            "bigm_algorithm": "adam",
            "nb_iter": 600,
            "cut_frequency": 450,
            "max_cuts": 12,
            "cut_add": 2,
            "betas": [0.9, 0.999],
            "initial_step_size": 1e-2,
            "final_step_size": 1e-5,
            "init_params": {
                "nb_outer_iter": 1000,
                "initial_step_size": 1e-2,
                "final_step_size": 1e-4,
                "betas": [0.9, 0.999],
                "larger_irl_if_naive_init": true
            },
            "restrict_factor": 1.5
        },
        "auto_iters": true,
        "batch_size": 30,
        "hard_overhead": 100
      }
    ],
    "do_ubs": false,
    "parent_init": true
  },

  "ibs": {
    "use_lb": true,
    "tight_ib": null,
    "fixed_ib": true,
    "joint_ib": false
  },

  "upper_bounding": {
    "ub_method": "mi_fgsm",
    "adv_params": {
      "iters": 500,
      "lr_tensor": true,
      "num_adv_ex": 100,
      "check_adv": 1,
      "mu_tensor": true,
      "decay_tensor": true
    }
  },

  "branching": {
    "heuristic_type": "FSB",
    "max_domains": 6000,
    "bounding": {
      "bounding_algorithm": "propagation",
      "params": {
        "best_among": ["KW", "crown"]
      },
      "type": "best_prop",
      "max_solver_batch": 12000
    }
  },

  "bab": {
    "max_cpu_subdomains": 5e4
  }
}
